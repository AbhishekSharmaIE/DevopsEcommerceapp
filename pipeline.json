{
    "pipeline": {
        "name": "ecommerce-app-pipeline",
        "roleArn": "arn:aws:iam::052188048367:role/CodePipelineServiceRole-ecommerce-app",
        "artifactStore": {
            "type": "S3",
            "location": "ecommerce-app-pipeline-artifacts"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "provider": "CodeStarSourceConnection",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "BranchName": "main",
                            "ConnectionArn": "arn:aws:codestar-connections:eu-west-1:052188048367:connection/fe7ebacb-e62e-4810-adfd-98ded3e1fa16",
                            "FullRepositoryId": "AbhishekSharmaIE/DevopsEcommerceapp"
                        },
                        "outputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "inputArtifacts": [],
                        "region": "eu-west-1",
                        "namespace": "SourceVariables"
                    }
                ]
            },
            {
                "name": "BuildAndDeploy",
                "actions": [
                    {
                        "name": "BuildAndDeploy",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "ecommerce-app-build"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "region": "eu-west-1",
                        "namespace": "BuildVariables"
                    }
                ]
            }
        ],
        "version": 2
    }
} 